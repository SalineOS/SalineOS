MODULE    = wordpad.exe
APPMODE   = -mwindows -mno-cygwin
IMPORTS   = comdlg32 uuid ole32 shell32 user32 gdi32 advapi32 comctl32
EXTRAINCL = -I$(top_srcdir)/include/msvcrt
EXTRADEFS = -DNO_LIBWINE_PORT
MODCFLAGS = @BUILTINFLAG@

C_SRCS = \
	olecallback.c \
	print.c \
	registry.c \
	wordpad.c

RC_SRCS = wordpad.rc
PO_SRCS = wordpad.rc

SVG_SRCS = \
	formatbar.svg \
	toolbar.svg \
	wordpad.svg

@MAKE_PROG_RULES@
